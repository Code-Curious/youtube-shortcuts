{
    "manifest_version": 2,

    "name": "Youtubeâ„¢ Shortcuts",
    "version": "1.1.0",
    "description": "Focus on the Youtube player with a the press of a button",
    "homepage_url": "https://daniwa.github.io",
    "options_page": "options.html",
    "icons": {
        "16": "imgs/16.png",
        "48": "imgs/48.png",
        "128": "imgs/128.png"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/*"],
            "js": [
                    "injected.js"
                ]
        }
    ]
}
